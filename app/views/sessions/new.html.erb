<h1>Sign in</h1>

<form role="form" action="/sessions" method="get">
	<div class="form-group">
		<label for="session_email">User Name</label><br />
		<input id="session_email" name="session[email]" size="30" type="text" />
	</div>
	<div class="form-group">
		<label for="session_password">Password</label><br />
		<input id="session_password" name="session[password]" size="30"
		type="password" />
	</div>
	<div class="actions">
		<input id="session_submit" class="btn btn-primary" name="commit" type="submit" value="Sign in" />
	</div>




	
</form>